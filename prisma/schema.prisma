generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transactions {
  id             String  @id @default(uuid())
  from           String
  to             String
  amount         Int
  suspicous      Boolean @default(false)
  transaction_id String  @unique

  @@map("transactions")
}

model InvalidTransactions {
  id        String @id @default(uuid())
  from      String
  to        String
  amount    Int
  reason    String
  file_name String

  @@map("invalid_transactions")
}
